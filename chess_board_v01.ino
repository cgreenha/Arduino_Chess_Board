/**/#include "square.h"#include "board.h"Board gameBoard;void setup() {  Serial.begin(9600);}void loop() {  printBoard(gameBoard);  gameBoard.move(1,1,3,1);  delay(2500);}void printBoard(Board b){  Serial.println();  Serial.println();  for(int y=0; y<8; y++){    for(int x=0; x<8; x++){      Serial.print(b.getTeam(x,y));      Serial.print(b.getPiece(x,y));      Serial.print(" ");    }    Serial.println();  }}